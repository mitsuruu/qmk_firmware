        "driver": "wear_leveling",
        "wear_leveling": {
            "backing_size": 8192,
            "driver": "spi_flash"
        }
    },
    "host": {
        "default": {
            "nkro": true
        }
    },
    "features": {
        "bootmagic": true,
        "command": false,
        "console": false,
        "deferred_exec": true,
        "extrakey": true,
        "mousekey": true,
        "nkro": true,
        "haptic": false,
        "rgb_light": false,
        "rgb_matrix": true
    },
    "tapping": {
        "term_per_key": true
    },
    "keycodes": [
        { "key": "KC_USB" },
        { "key": "KC_BT1" },
        { "key": "KC_BT2" },
        { "key": "KC_BT3" },
        { "key": "KC_2G4" },
        { "key": "KC_NXT" }
